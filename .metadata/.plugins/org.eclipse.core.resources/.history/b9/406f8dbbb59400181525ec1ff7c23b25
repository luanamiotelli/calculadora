package CalculadoraProjeto;


public class Calcula {
	public int soma(int nm1, int nm2) {
		return nm1 + nm2;
	}
	public int subtrai(int nm1, int nm2){
		return nm1 - nm2;
	}
	public int divide(int nm1,int nm2){
		return nm1 / nm2;
	}
	public int multiplica(int nm1, int nm2){
		return nm1 * nm2;
	}
}
